{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/gatsby"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint",
        "strict": true
      }
    },
    "@nrwl/gatsby": {
      "application": {
        "style": "css"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "formaxui-components": {
      "root": "libs/formaxui/components",
      "sourceRoot": "libs/formaxui/components/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formaxui/components",
            "tsConfig": "libs/formaxui/components/tsconfig.lib.json",
            "project": "libs/formaxui/components/package.json",
            "entryFile": "libs/formaxui/components/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/formaxui/components/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/formaxui/components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formaxui/components"],
          "options": {
            "jestConfig": "libs/formaxui/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/formaxui/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/formaxui-components",
            "config": {
              "configFolder": "libs/formaxui/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "formaxui-components-e2e": {
      "root": "apps/formaxui-components-e2e",
      "sourceRoot": "apps/formaxui-components-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/formaxui-components-e2e/cypress.json",
            "tsConfig": "apps/formaxui-components-e2e/tsconfig.e2e.json",
            "devServerTarget": "formaxui-components:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "formaxui-components:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/formaxui-components-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "formaxui-components-src-lib-button": {
      "root": "libs/formaxui/components/src/lib/button",
      "sourceRoot": "libs/formaxui/components/src/lib/button/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formaxui/components/src/lib/button",
            "tsConfig": "libs/formaxui/components/src/lib/button/tsconfig.lib.json",
            "project": "libs/formaxui/components/src/lib/button/package.json",
            "entryFile": "libs/formaxui/components/src/lib/button/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/formaxui/components/src/lib/button/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/formaxui/components/src/lib/button/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formaxui/components/src/lib/button"],
          "options": {
            "jestConfig": "libs/formaxui/components/src/lib/button/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/formaxui/components/src/lib/button/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/formaxui-components-src-lib-button",
            "config": {
              "configFolder": "libs/formaxui/components/src/lib/button/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "formaxui-components-src-lib-window": {
      "root": "libs/formaxui/components/src/lib/window",
      "sourceRoot": "libs/formaxui/components/src/lib/window/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formaxui/components/src/lib/window",
            "tsConfig": "libs/formaxui/components/src/lib/window/tsconfig.lib.json",
            "project": "libs/formaxui/components/src/lib/window/package.json",
            "entryFile": "libs/formaxui/components/src/lib/window/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/formaxui/components/src/lib/window/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/formaxui/components/src/lib/window/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formaxui/components/src/lib/window"],
          "options": {
            "jestConfig": "libs/formaxui/components/src/lib/window/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/formaxui/components/src/lib/window/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/formaxui-components-src-lib-window",
            "config": {
              "configFolder": "libs/formaxui/components/src/lib/window/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "formaxui-types": {
      "root": "libs/formaxui/types",
      "sourceRoot": "libs/formaxui/types/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/formaxui/types",
            "tsConfig": "libs/formaxui/types/tsconfig.lib.json",
            "project": "libs/formaxui/types/package.json",
            "entryFile": "libs/formaxui/types/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/formaxui/types/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/formaxui/types/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/formaxui/types"],
          "options": {
            "jestConfig": "libs/formaxui/types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
