{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../../../../../../../../dist/out-tsc",
    "module": "commonjs",
    "types": ["jest", "node"],
    "noImplicitAny": true,
    "target": "es5",
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "incremental": false
  },
  "include": [
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.spec.js",
    "**/*.spec.jsx",
    "**/*.d.ts"
  ]
}
